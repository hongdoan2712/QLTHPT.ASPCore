@model QLTHPT.Models.Thoikhoabieu

@{
    ViewBag.Title = "Delete";
}

<div class="data-table-area mg-b-15">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                <div class="sparkline13-list shadow-reset">
                    <div class="sparkline13-hd">
                        <div class="main-sparkline13-hd">
                            <h4>
                                <strong>
                                    Xóa
                                </strong>
                            </h4>

                        </div>
                    </div>
                    <h5>Bạn có chắc muốn xóa ?</h5>
                    <div class="sparkline10-graph">
                        <div class="card-body">
                            <table id="bootstrap-data-table" class="table table-striped table-bordered">
                                <thead>
                                    <tr>
                                        <th>
                                            Mã TKB
                                        </th>
                                        <th>
                                            Cán Bộ
                                        </th>
                                        <th>Học Kỳ</th>
                                        <th>Lớp</th>
                                        <th>Môn Học</th>
                                        <th>Tiết Học</th>
                                        <th>Thứ</th>
                                    </tr>
                                    <tr>

                                        <td>@Html.DisplayFor(model => model.TkbMa)</td>
                                        <td>
                                            @Html.DisplayFor(model => model.CanboCbMa)
                                        </td>
                                        <td>@Html.DisplayFor(model => model.HockyHkMa)</td>
                                        <td>
                                            @Html.DisplayFor(model => model.LopLopMa)
                                        </td>
                                        <td>@Html.DisplayFor(model => model.MonhocMhMa)</td>
                                        <td>@Html.DisplayFor(model => model.TiethocThMa)</td>
                                        <td>@Html.DisplayFor(model => model.ThuThuMa)</td>
                                    </tr>
                                </thead>

                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-actions no-color">
        <input type="submit" value="Xóa" class="btn btn-default" /> |
        <a href="@Url.Action("Index", "Thoikhoabieu")" class=" btn btn-custon-rounded-three btn-primary"><i class="fa fa-undo"></i></a>
    </div>
}

